for i in range(1,6 , +1):
    print(i)




for i in range(0,6 , -1):
    print(i)





for i in range(5, 16, 2):
    print(i)


sum = 0

for i in range(1, 6):
    sum = sum + i
print(sum)







sum = 0
count = 0

for i in range(5, 8):
    count = count = 1
    sum = sum + 1

averege = sum / count 

print(averege)


